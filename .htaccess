AddDefaultCharset UTF-8
RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]

RewriteRule ^View-Menu/id/([0-9]+)/?$ View-Menu.php?id=$1 [NC,QSA,L]

RewriteEngine On
RewriteCond %{HTTP_HOST} ^mietheld\.at [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.mietheld.at$1 [R,L]